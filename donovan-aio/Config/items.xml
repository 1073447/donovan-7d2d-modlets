<configs>
  <!-- Included from donovan-betterbandages -->
  <set xpath="items/item[@name='medicalFirstAidBandage']/property[@name='UnlockedBy']/@value">perkPhysician,medicalFirstAidBandageSchematic</set>
  <set xpath="items/item[@name='medicalFirstAidKit']/property[@name='UnlockedBy']/@value">perkPhysician,medicalFirstAidKitSchematic</set>
  <!-- Included from donovan-betterpowertools -->
  <!-- Chainsaw -->
  <set xpath="items/item[@name='meleeToolChainsaw']/effect_group[@name='meleeToolChainsaw']/passive_effect[@name='MaxRange']/@value">3.5</set>
  <set xpath="items/item[@name='meleeToolChainsaw']/effect_group[@name='meleeToolChainsaw']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">20</set>
  <set xpath="items/item[@name='meleeToolChainsaw']/effect_group[@name='meleeToolChainsaw']/passive_effect[@name='BlockDamage' and @operation='base_set']/@value">50</set>
  <set xpath="items/item[@name='meleeToolChainsaw']/effect_group[@name='meleeToolChainsaw']/passive_effect[@name='MagazineSize' and @operation='base_set']/@value">500</set>
  <append xpath="items/item[@name='meleeToolChainsaw']/effect_group[@name='meleeToolChainsaw']">
    <passive_effect name="HarvestCount" operation="base_set" value=".35" tags="butcherHarvest"/>
  </append>
  <!-- Auger -->
  <set xpath="items/item[@name='meleeToolAuger']/effect_group[@name='meleeToolAuger']/passive_effect[@name='MaxRange']/@value">3.5</set>
  <set xpath="items/item[@name='meleeToolAuger']/effect_group[@name='meleeToolAuger']/passive_effect[@name='BlockDamage' and @operation='base_set']/@value">50</set>
  <set xpath="items/item[@name='meleeToolAuger']/effect_group[@name='meleeToolAuger']/passive_effect[@name='MagazineSize' and @operation='base_set']/@value">500</set>
  <!-- Nailgun -->
  <set xpath="items/item[@name='gunToolNailgun']/property[@class='Action1']/property[@name='Delay']/@value">0.25</set>
  <!-- Included from donovan-betterspears -->
  <set xpath="items/item[@name='meleeThrownSpearStone']/effect_group[@name='meleeThrownSpearStone']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">11.33</set>
  <set xpath="items/item[@name='meleeThrownSpearStone']/effect_group[@name='Throw']/passive_effect[@name='EntityDamage']/@value">3.14</set>
  <set xpath="items/item[@name='meleeThrownSpearIron']/effect_group[@name='meleeThrownSpearIron']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">14.26</set>
  <set xpath="items/item[@name='meleeThrownSpearIron']/effect_group[@name='Throw']/passive_effect[@name='EntityDamage']/@value">3.28</set>
  <set xpath="items/item[@name='meleeThrownSpearSteel']/effect_group[@name='meleeThrownSpearSteel']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">23.25</set>
  <set xpath="items/item[@name='meleeThrownSpearIron']/effect_group[@name='Throw']/passive_effect[@name='EntityDamage']/@value">3.56</set>
  <!-- Included from donovan-lessgrind -->
  <!-- Crafting time -->
  <set xpath="items/item[starts-with(@name, 'resource')]/property[@name='CraftingIngredientTime']/@value">0.1</set>
  <set xpath="items/item[@name='resourceForgedIron']/property[@name='CraftingIngredientTime']/@value">2</set>
  <set xpath="items/item[@name='resourceForgedSteel']/property[@name='CraftingIngredientTime']/@value">5</set>
  <set xpath="items/item[@name='toolAnvil']/effect_group/passive_effect[@name='CraftingTime']/@value">-.5</set>
  <!-- Melting time -->
  <set xpath="items/item[@name='unit_iron']/property[@name='MeltTimePerUnit']/@value">0.05</set>
  <set xpath="items/item[@name='toolBellows']/effect_group/passive_effect[@name='CraftingSmeltTime']/@value">-.5</set>
  <!-- Included from donovan-megastacks -->
  <set xpath="/items/item[starts-with(@name, 'ammo')]/property[@name='Stacknumber']/@value">1000</set>
  <!-- Ammo -->
  <set xpath="/items/item[starts-with(@name, 'medical')]/property[@name='Stacknumber']/@value">50</set>
  <!-- Medical -->
  <set xpath="/items/item[starts-with(@name, 'drug')]/property[@name='Stacknumber']/@value">100</set>
  <!-- Drugs -->
  <set xpath="/items/item[starts-with(@name, 'food')]/property[@name='Stacknumber']/@value">100</set>
  <!-- Food -->
  <set xpath="/items/item[starts-with(@name, 'foodCrop')]/property[@name='Stacknumber']/@value">1000</set>
  <!-- Crops -->
  <set xpath="/items/item[starts-with(@name, 'drink')]/property[@name='Stacknumber']/@value">500</set>
  <!-- Drinks -->
  <set xpath="/items/item[starts-with(@name, 'resource')]/property[@name='Stacknumber']/@value">30000</set>
  <!-- Resources -->
  <set xpath="/items/item[starts-with(@name, 'resourceCrop')]/property[@name='Stacknumber']/@value">500</set>
  <!-- Resources -->
  <set xpath="/items/item[starts-with(@name, 'tool')]/property[@name='Stacknumber']/@value">25</set>
